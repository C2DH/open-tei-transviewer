   <text xml:id="transviewer_sample_3" decls="#transcr-facs" type="sample" xml:lang="en">
      <body>
         <div>
            <pb n="001" xml:id="transviewer-sample_001" corresp="#surf_001"/>
            <fw type="header" corresp="#doc_auth">Author of the document</fw>
            <fw type="header" corresp="#confid_stamp" rendition="red">CONFIDENTIAL</fw>
            <fw type="header" corresp="#doc_vers">FINAL VERSION</fw>
            <fw type="header" corresp="#doc_ref">DocRef</fw>
            <fw type="header" corresp="#orig_lang">Original English</fw>
            <fw type="header" corresp="#orig_date">
               <date>October 17<hi rend="superscript">th</hi>, 2017</date>
            </fw>
            <head>TEI Transviewer Sample</head>
            <head>Transcription and facsimile</head>
            <p>[Text] <name type="org">organisation name 1</name> [text] <name type="event">event name 1</name> [text] <name type="place">place name 1</name>.</p>
            <p>[Text]</p>
            <div>
               <head>Section 1</head>
               <p>[Text]<hi rend="superscript"><ref type="noteAnchor" target="#fn1" xml:id="na1">1</ref></hi></p>
               <p>[Text]</p>
               <p>[Text]</p>
               <p>[Text]</p>
               <p>[Text] <name type="person">person name 1</name>, <name type="function">position 1</name> [text] <name type="org">organisation name 2</name> [text text text text text <lb/>text text text text text text text text text text text]<hi rend="superscript"><ref type="noteAnchor" target="#fn2" xml:id="na2">2</ref></hi></p>
               <p>[Text]</p>
               <p>[Text]</p>
               <p>[Text text text text text text text text text text text text text text text text text <lb/>text text text text text text text text] <name type="product">product 1</name>, <name type="product">product 2</name>, <name type="product">product 3</name> [text text text]</p>
               <p>[Text]</p>
               <p>[Text]</p>
               <p>[Text] <name type="event">event name 2</name> [text] <date>date 2</date> [text] <name type="place">place name 2</name> [text text text text text <lb/>text text text text text text text text text text text text text text text text <lb/>text text text text text text text text text text text text text text text]</p>
               <p>[Text]</p>
               <p>[Text]</p>
               <p>[Text]</p>
               <p>[Text text text text text text text text text text text text text text text text <lb/>text text text text text text text text text text text text]</p>
               <fw type="footer">
                  <note type="footnote" xml:id="fn1">
                     <hi rend="superscript"><ref target="#na1">1</ref></hi> Footnote 1<lb/>
                  </note>
                  <note type="footnote" xml:id="fn2">
                     <hi rend="superscript"><ref target="#na2">2</ref></hi> Footnote 2<lb/>
                  </note>
               </fw>
               <fw type="footer" corresp="#confid_stat">CONFIDENTIAL</fw>
            </div>
            <div>
               <pb n="002" xml:id="transviewer-sample_002" corresp="#surf_002"/>
               <fw type="pageNum">- 2 -</fw>
               <fw type="header" corresp="#doc_ref">DocRef</fw>
               <head>Section 2</head>
               <p>[Text]</p>
               <p>[Text] <name type="person">person name 2</name>, <name type="function">position 2</name>, and <name type="person">person name 1</name> [Text]</p>
               <p>[Text]</p>
               <div>
                  <head>Section 2.1</head>
                  <p>[Text text text text text text text text text text text text text text text text text text <lb/>text text text text text text text text] <name type="product">product 1</name>, <name type="product">product 2</name>, <name type="product">product 4</name> [text text text]</p>
                  <p>[Text] <name type="org">organisation name 3</name>, <name type="org">organisation name 4</name>. [text] <name type="org">organisation name 1</name> <lb/>[text]</p>
                  <p>[Text]</p>
                  <p>[Text] <date>date 3</date> [text]</p>
                  <p>[Text]</p>
                  <p>[Text] <name type="event">event name 3</name> [text] <date>date 2</date> [text]</p>
                  <p>[Text]</p>
                  <p>[Text]</p>
                  <p>[Text]</p>
               </div>
               <div>
                  <head>Section 2.2</head>
                  <p>[Text]</p>
                  <p>[Text] <name type="place">place name 3</name> [text] <name type="place">place name 4</name> [text] <name type="person">person name 3</name> [text text text text <lb/>text text text text text text text text text text text text text text text text text <lb/>text text text text text text text]</p>
                  <p>[Text]</p>
                  <p>[Text]</p>
                  <p>[Text] <name type="place">place name 2</name> [text]</p>
                  <p>[Text]</p>
                  <p>[Text] <date>date 3</date> [text] <name type="event">event name 4</name> [text] </p>
                  <p>[Text]</p>
                  <p>[Text] <name type="org">organisation name 5</name> [text]</p>
                  <p>[Text]</p>
                  <fw type="footer" corresp="#confid_stat">CONFIDENTIAL</fw>
               </div>
               <div>
                  <pb n="002" xml:id="transviewer-sample_003" corresp="#surf_003"/>
                  <fw type="pageNum">- 3 -</fw>
                  <fw type="header" corresp="#doc_ref">DocRef</fw>
